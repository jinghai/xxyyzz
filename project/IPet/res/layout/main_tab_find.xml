<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res/com.ipet"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <com.ipet.android.ui.common.SimpleTitleBar
        android:id="@id/main_find_titlebar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/main_find_title"
        app:title_left_res_visibility="invisible"
        app:title_right_res_visibility="invisible" />
    
    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none" >

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingTop="2dp"
            android:src="@drawable/x2" />

    </ScrollView>
</LinearLayout>
